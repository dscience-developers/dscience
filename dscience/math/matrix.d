class Stoichiometric{
    private size_t maxRaw       = 0;
    private size_t maxColumn    = 0;
    private double[][] matrix   = null;

    public this(size_t maxRaw, size_t maxColumn){
        this.maxRaw             = maxRaw;
        this.maxColumn          = maxColumn;
        this.matrix             = new double[][](maxRaw, maxColumn);
    }

    public this(size_t maxRaw, size_t maxColumn, double[][] matrix){
        this.maxRaw             = maxRaw;
        this.maxColumn          = maxColumn;
        this.matrix             = matrix;
    }

    public void add(double value, size_t column,size_t raw){
        matrix[raw][column] = value;
    }

    public void  opIndexAssign(double value, size_t column,size_t raw){
        add(value,column,raw);
    }

    public void opAddAssign(double value, size_t column,size_t raw){
        matrix[raw][column] += value;
    }


    public double opIndex(size_t raw, size_t column){
        return matrix[raw][column];
    }

    public Stoichiometric dup(){
        return new Stoichiometric(maxRaw,maxColumn, matrix.dup);
    }
}
