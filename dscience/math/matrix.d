/**
 * normalize
 * Params:
 *  matrix      = matrix to analyze
 *  label       = name / position to each column ( ["A":0, "C":1, "G":2, "T":3] )
 *  background  = letters frequencies (in whole genome by example)
 * Returns:
 *  a normalized matrix, log2(freq b position i / freq b assembly)
 * Example:
 *  double[][]      matrix              = loadMatrixFile!double( "pwm.mat" );
 *  string[size_t]  background          = ["A":0.25, "T":0.25, "G":0.25, "C":0.25];
 *  double[][]      matrixNormalized    = normalize!double( matrix, background );
 */
T[][] normalize(T)( ref T[][] matrix, size_t[string] label, T[string] background ){
    T[][] matrixNormalized = new T[][]( matrix.length, matrix[0].length );  // TODO: check matrix length
    foreach( letter; background.keys() ){
        size_t column = label[letter];                                      // get in which column is located corresponding letter
        foreach( size_t line; 0 ..  matrix.length ){
            real tmp = matrix[line][column] / background[letter];
            if( tmp == 0 )
                matrixNormalized[line][column] = 0;
            else
                matrixNormalized[line][column] = log2( tmp );
        }
    }
    return matrixNormalized;
}
/**
 * information
 * i.e Shannon and Weaver (1949)
 * Returns:
 *  Information value I at position j from a signal
 *  Params:
 *      matrix      = a normalized position weight matrix
 *      label       = name / position to each column ( ["A":0, "C":1, "G":2, "T":3] )
 *      background  = ["A":0.25, "T":0.25, "G":0.25, "C":0.25];
 */
T[] information( T )( ref T[][] motif, size_t[string] label, T[string] backgroung){
    size_t  numberUniqueLetter  = backgroung.keys().length;
    T[]     result              = new T[]( numberUniqueLetter );    // Create result array at right length
    foreach( string letter, size_t position; label ){               // Iterate for each letter by example for DNA ACGT -> loop 0 to 3 }
        T[] tmp                 = new T[]( motif.length );          // Create a tmp array where will contains result for this column
        foreach( size_t motifPosition; 0..motif.length ){           // Get freq for each letter a this position
            real logCompute1    = ( motif[motifPosition][position] > 0) ? log2( motif[motifPosition][position] )    : 0;
            real logCompute2    = ( backgroung[letter] > 0)             ? log2(backgroung[letter])                  : 0;
            tmp[motifPosition]  = logCompute1 - logCompute2;
        }
        result[position] = reduce!"a+b"(tmp);                       // sum of this elements
    }
    return result;
}

